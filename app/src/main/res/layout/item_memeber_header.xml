<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/view_margin_8dp"
    android:paddingBottom="@dimen/view_margin_8dp">



    <CheckBox
        android:id="@+id/check"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_marginRight="@dimen/view_margin_8dp"
        android:button="@drawable/checkbox_btn2"
        android:visibility="gone"/>

    <ImageView
        android:id="@+id/delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_marginRight="@dimen/view_margin_8dp"
        android:src="@mipmap/delete_checked"
        android:visibility="gone"/>

    <com.biu.modulebase.binfenjiari.widget.CircleImageView
        android:id="@+id/header"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:src="@mipmap/header_pic"/>

    <TextView
        android:id="@+id/admin_type"
        android:layout_width="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_height="wrap_content"
        android:background="@drawable/selector_rr_orange_5_rest"
        android:enabled="true"
        android:textSize="@dimen/text_size_12"
        android:padding="@dimen/view_margin_4dp"
        android:clickable="true"
        android:textColor="@color/white"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:text="管理员" />
    <FrameLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/child_name"
        android:layout_width="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_height="wrap_content"
        android:textSize="@dimen/text_size_16"
        android:layout_marginLeft="@dimen/view_margin_8dp"
        android:text="Lee" />

        <TextView
            android:id="@+id/me"
            android:layout_width="wrap_content"
            android:layout_gravity="center_vertical|right"
            android:layout_height="wrap_content"
            android:textSize="@dimen/text_size_14"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:text="我"
            android:visibility="gone"/>
    </FrameLayout>



</LinearLayout>
