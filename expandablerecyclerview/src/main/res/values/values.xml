<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PatchedRecyclerView">
        <attr name="hasFixedSize" format="boolean|reference"/>
        <attr name="nestedScrollingEnabled" format="boolean|reference"/>
        <!--RecyclerView 的布局方向 -->
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0x0"/>
            <enum name="vertical" value="0x1"/>
        </attr>
    </declare-styleable>
</resources>